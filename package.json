    "@types/node": "^20.6.3",
    "eslint": "^8.49.0",
    "nodemon": "^3.0.1",
    "prettier": "^3.0.3",
    "typescript": "^5.2.2"
  },
  "scripts": {
    "start": "node build/index.js",
    "compile": "tsc && node build/index.js",
    "build": "tsc",
    "dev": "nodemon -e .ts --exec \"yarn compile\"",
    "lint": "eslint --ext .js,.ts .",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\"",
    "prepare": "husky install"
  }
